\">\n          <div className=\"wrapper\">\n            <div className=\"tit\">\n              <p className=\"img\">\n                <img src=\"/images/ico/tom2_logo.png\" alt=\"\" />\n              </p>\n              <p className=\"txt\">\n                <img src=\"/images/ico/ico_tit.png\" alt=\"\" />\n              </p>\n              <h3>Stake TOM LP tokens to get TOM2!</h3>\n            </div>\n          </div>\n        </div>\n        <div id=\"jsHomeMyBalance\" className=\"main_btm wrapper\">\n          <div className=\"figure\">\n            <div className=\"balance\">\n              <p>My TOM2 Balance</p>\n              <strong>{balanceObj.myTomBalance}</strong>\n              {/* <div className=\"pend\">\n                            <p>Pending harvest</p>\n                            <span>0.00000000</span>\n                        </div> */}\n            </div>\n            <div className=\"supply\">\n              <p>Current Total supply</p>\n              <strong>{balanceObj.totalSupply}</strong>\n              {/* <div className=\"pend\">\n                            <p>APY</p>\n                            <span>10.00000000 TOM2</span>\n                        </div> */}\n            </div>\n          </div>\n          <Link to=\"/farm\" className=\"main_btn\">\n            See the Touch of Midas Farm\n          </Link>\n        </div>\n      </div>\n      {showModal ? (\n        <div id=\"will_pop\" className=\"popup_wrap\">\n          <div\n            className=\"popup-close\"\n            onClick={() => setShowModal(false)}\n          ></div>\n          <div className=\"popup\">\n            <a\n              href=\"https://go.latoken.com/64d\"\n              target=\"_blank\"\n              rel=\"noopener noreferrer\"\n              alt=\"latoken\"\n            >\n              <figure className=\"popup-figure\">\n                <img src=\"/images/ico/tom2latoken.jpg\" alt=\"latoken\" />\n              </figure>\n            </a>\n          </div>\n        </div>\n      ) : null}\n    </>\n  );\n};\n\nexport default Home;\n","/Users/fri13th/projects/tom2/src/utils/metaMask.js",[],"/Users/fri13th/projects/tom2/src/config.js",[],"/Users/fri13th/projects/tom2/src/Farm/FarmStake.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Exchange.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Pool/ImportPool.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Pool/AddLiquidity.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Pool/RemoveLiquidity.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Pool.js",[],"/Users/fri13th/projects/tom2/src/Farm/StakeModal.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Pool/TokenListModal.js",[],"/Users/fri13th/projects/tom2/src/Exchange/LoadingModal.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Swap/Swap.js",[],"/Users/fri13th/projects/tom2/src/assets/Responsive.js",[],"/Users/fri13th/projects/tom2/src/Exchange/ConfirmModal.js",[],"/Users/fri13th/projects/tom2/src/Exchange/Swap/SwapConfirm.js",[],"/Users/fri13th/projects/tom2/src/Global/HelpBox.js",[],"/Users/fri13th/projects/tom2/src/utils/hook/useInput.js",[],["123","124"],{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","severity":1,"message":"130","line":12,"column":7,"nodeType":"131","messageId":"132","endLine":12,"endColumn":12},{"ruleId":"129","severity":1,"message":"133","line":21,"column":7,"nodeType":"131","messageId":"132","endLine":21,"endColumn":22},{"ruleId":"129","severity":1,"message":"134","line":36,"column":7,"nodeType":"131","messageId":"132","endLine":36,"endColumn":17},{"ruleId":"129","severity":1,"message":"135","line":42,"column":7,"nodeType":"131","messageId":"132","endLine":42,"endColumn":15},{"ruleId":"125","replacedBy":"136"},{"ruleId":"127","replacedBy":"137"},"no-native-reassign",["138"],"no-negated-in-lhs",["139"],"no-unused-vars","'Title' is assigned a value but never used.","Identifier","unusedVar","'DescriptionWrap' is assigned a value but never used.","'